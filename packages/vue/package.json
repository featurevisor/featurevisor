{
  "name": "@featurevisor/vue",
  "version": "2.19.0",
  "description": "Vue.js package for Featurevisor",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "transpile": "rimraf lib && tsc --project tsconfig.esm.json",
    "dist": "webpack --config ./webpack.config.js",
    "build": "npm run transpile && npm run dist",
    "test": "jest --config jest.config.js --verbose --coverage ./src/*.spec.ts"
  },
  "author": {
    "name": "Fahad Heylaal",
    "url": "https://fahad19.com"
  },
  "homepage": "https://featurevisor.com",
  "keywords": [
    "featurevisor",
    "feature",
    "features",
    "flags",
    "feature flags",
    "feature toggles",
    "feature management",
    "experimentation",
    "experiment",
    "experiments",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/featurevisor/featurevisor.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/featurevisor/featurevisor/issues"
  },
  "dependencies": {
    "@featurevisor/sdk": "2.19.0",
    "@featurevisor/types": "2.19.0",
    "vue": "^3.3.4"
  },
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@vue/test-utils": "^2.4.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.0.5"
  }
}
