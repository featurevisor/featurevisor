{
  "name": "@featurevisor/site",
  "version": "0.7.0",
  "description": "Static site generator for Featurevisor",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "echo 'not linting in this package yet'",
    "transpile": "rimraf lib && tsc --project tsconfig.cjs.json",
    "copy:html": "cp ./fe/index.html ./dist/index.html",
    "dist:fe": "webpack --config ./webpack.fe.config.js",
    "dist": "npm run dist:fe",
    "build": "npm run dist && npm run copy:html && npm run transpile",
    "server:local": "http-server ./dist",
    "test": "echo 'Nothing to test in types package'"
  },
  "author": {
    "name": "Fahad Heylaal",
    "url": "https://fahad19.com"
  },
  "homepage": "https://featurevisor.com",
  "keywords": [
    "featurevisor",
    "feature",
    "features",
    "flags",
    "feature flags",
    "feature toggles",
    "feature management",
    "experimentation",
    "experiment",
    "experiments"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fahad19/featurevisor.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/fahad19/featurevisor/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.29",
    "bulma": "^0.9.4",
    "css-loader": "^6.7.3",
    "style-loader": "^3.3.2"
  },
  "dependencies": {
    "http-server": "^14.1.1"
  }
}
