{
  "name": "@featurevisor/types",
  "version": "0.48.0",
  "description": "Common Typescript types for Featurevisor",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "transpile": "rimraf lib && tsc --project tsconfig.esm.json",
    "dist": "webpack --config ./webpack.config.js",
    "build": "npm run transpile && npm run dist",
    "test": "echo 'Nothing to test in types package'",
    "format": "prettier . --check --cache --log-level=warn",
    "lint": "eslint . --cache",
    "format:fix": "prettier . --write --cache --log-level=warn",
    "lint:fix": "eslint . --fix --cache"
  },
  "author": {
    "name": "Fahad Heylaal",
    "url": "https://fahad19.com"
  },
  "homepage": "https://featurevisor.com",
  "keywords": [
    "featurevisor",
    "feature",
    "features",
    "flags",
    "feature flags",
    "feature toggles",
    "feature management",
    "experimentation",
    "experiment",
    "experiments"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/featurevisor/featurevisor.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
