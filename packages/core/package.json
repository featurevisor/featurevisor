{
  "name": "@featurevisor/core",
  "version": "1.19.0",
  "description": "Core package of Featurevisor for Node.js usage",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "transpile": "rimraf lib && tsc --project tsconfig.cjs.json",
    "dist": "echo 'Nothing to dist here'",
    "build": "npm run transpile",
    "test": "jest --config jest.config.js --verbose --coverage",
    "format": "prettier . --check --cache --loglevel=warn",
    "lint": "eslint . --cache",
    "format:fix": "prettier . --write --cache --loglevel=warn",
    "lint:fix": "eslint . --fix --cache"
  },
  "author": {
    "name": "Fahad Heylaal",
    "url": "https://fahad19.com"
  },
  "homepage": "https://featurevisor.com",
  "keywords": [
    "featurevisor",
    "feature",
    "features",
    "flags",
    "feature flags",
    "feature toggles",
    "feature management",
    "experimentation",
    "experiment",
    "experiments"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/featurevisor/featurevisor.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/featurevisor/featurevisor/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@featurevisor/sdk": "^1.17.0",
    "@featurevisor/site": "^1.18.1",
    "@featurevisor/types": "^1.3.0",
    "axios": "^1.3.4",
    "js-yaml": "^4.1.0",
    "mkdirp": "^2.1.3",
    "tar": "^6.1.13",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/tar": "^6.1.4"
  }
}
